{
  "name": "vue-watch",
  "version": "1.0.0",
  "description": "Renderless component that fires events when a property on its underlying component changes",
  "author": "Venkat Peri <venkatperi@gmail.com>",
  "main": "dist/VueWatch.min.js",
  "unpkg": "dist/VueWatch.min.js",
  "browser": "dist/umd/VueWatch.min.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src/**/*.{js,vue}",
    "build": "poi build --format cjs --dist dist; poi build --format umd --dist dist/umd",
    "test": "poi test",
    "test:watch": "poi test --watch",
    "test:cov": "poi test --coverage",
    "clean": "rm -rf dist",
    "prepare": "npm run clean; npm run build"
  },
  "keywords": [
    "vue",
    "vuejs",
    "component"
  ],
  "files": [
    "src/",
    "dist/"
  ],
  "dependencies": {
    "vue": "^2.5.3"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-plugin-vue": "beta",
    "karma-chai": "^0.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.5.0",
    "node-sass": "^4.9.3",
    "poi": "^9.5.2",
    "poi-preset-karma": "^9.2.1",
    "sass-loader": "^7.1.0",
    "to-pascal-case": "^1.0.0",
    "typescript": "^2.6.1",
    "vue-webpack-loaders": "^1.0.6",
    "webpack": "^3.1.0"
  }
}
